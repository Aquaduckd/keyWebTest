{
  "name": "keywebtest",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=dist/bundle.js --format=esm --sourcemap",
    "dev": "esbuild src/main.ts --bundle --outfile=dist/bundle.js --format=esm --sourcemap --watch",
    "typecheck": "tsc --noEmit",
    "serve": "python3 -m http.server 8000"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "typescript": "^5.3.3"
  }
}

